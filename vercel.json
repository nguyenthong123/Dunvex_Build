{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "unsafe-none"
				},
				{
					"key": "X-Frame-Options",
					"value": "ALLOWALL"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/assets/(.*)",
			"destination": "/assets/$1"
		},
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}